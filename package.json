{
	"name": "vscode-dbuniit-xml-editor",
	"description": "XML Editor For DBUnit Dataset XML",
	"version": "0.0.1",
	"publisher": "onBass-naga",
	"repository": "https://github.com/onBass-naga/vscode-dbunit-xml-editor",
	"engines": {
		"vscode": "^1.74.0"
	},
	"activationEvents": [],
	"main": "./out/extension.js",
	"contributes": {
		"customEditors": [
			{
				"viewType": "dbunit.xml.editor",
				"displayName": "DBUnit XML Editor",
				"selector": [
					{
						"filenamePattern": "*.xml"
					}
				]
			}
		],
		"commands": [
			{
				"command": "extension.dbunitXmlEditor",
				"title": "DBUnit XML Editor"
			}
		],
		"menus": {
			"editor/title": [
				{
					"command": "extension.dbunitXmlEditor",
					"group": "navigation",
					"when": "editorLangId == xml"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^16.18.34",
		"@types/vscode": "^1.73.0",
		"@typescript-eslint/eslint-plugin": "^5.42.0",
		"@typescript-eslint/parser": "^5.42.0",
		"eslint": "^8.26.0",
		"typescript": "^5.1.3"
	}
}